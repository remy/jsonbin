{
  "name": "jsonbin",
  "version": "2.5.3",
  "description": "",
  "main": "lib/index.js",
  "private": true,
  "now": {
    "alias": "jsonbin.org",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "scripts": {
    "now-start": "node .",
    "postversion": "now && now alias",
    "start": "NODE_ENV=dev PORT=3001 nodemon . -w lib",
    "pretest": "NODE_ENV=test PORT=7331 node . & echo $! > .pid; sleep 1;",
    "test": "NODE_ENV=test PORT=7331 tap tests/*.test.js -Rspec; kill $(cat .pid); rm .pid",
    "deploy": "npm test && now && now alias"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-hbs": "^1.0.3",
    "express-session": "^1.14.2",
    "file-type": "^4.0.0",
    "http-status-codes": "^1.0.6",
    "inline-log": "^1.0.4",
    "json-merge-patch": "^0.2.3",
    "jsondiffpatch": "^0.2.4",
    "jsonwebtoken": "^7.3.0",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "mongoose": "^4.7.5",
    "morgan": "^1.7.0",
    "ms": "^0.7.2",
    "multer": "^1.2.1",
    "on-headers": "^1.0.1",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.2",
    "server-timings": "^1.0.4",
    "shortid": "^2.2.6",
    "std-loggly": "^1.0.0",
    "undefsafe": "1.3.1",
    "universal-analytics": "^0.4.8",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.7",
    "tap": "^8.0.1",
    "tap-only": "^0.0.5"
  }
}
